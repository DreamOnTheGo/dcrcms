<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title><{$classname}>_<{$web_name}></title>
<meta name="keywords" content="{pro.keywords}"/>
<meta name="description" content="{pro.description}"/>
<link href="<{$web_templeturl}>/<{$web_templetdir}>/css/common.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="<{$web_templeturl}>/<{$web_templetdir}>/js/jquery.js"></script>
<script type="text/javascript" src="<{$web_templeturl}>/<{$web_templetdir}>/js/jquery.cookie.js"></script>
<script type="text/javascript" src="<{$web_templeturl}>/<{$web_templetdir}>/js/jquery.treeview.js"></script>
<link href="<{$web_templeturl}>/<{$web_templetdir}>/css/jquery.treeview.css" rel="stylesheet" type="text/css" />
</head>
<body>
<div class="wrap">
	<{include file="header.html"}>
    <div class="con clearfix">
    	<div id="con_l">
			<{include file="left.html"}>
        </div>
    	<div id="con_main">
            <div class="products clearfix">
              <div class="title"><img src="<{$web_templeturl}>/<{$web_templetdir}>/images/products.gif" width="200" height="22" /></div>
              <div class="pro_position"><{$position}></div>
              <div class="productb clearfix"> <{section name=arr loop=$pro_list}>
                <dl>
                  <dd><a title="<{$pro_list[arr].title}>" href="<{$pro_list[arr].url}>" target=_blank><img src="<{$pro_list[arr].logo}>" width="180" height="140" /></a></dd>
                  <dt><a title="<{$pro_list[arr].title}>" href="<{$pro_list[arr].url}>" target=_blank><{$pro_list[arr].title}></a></dt>
                </dl>
                <{/section}></div>
            </div>
            <div class="clearfix tr p10"><{$fenye}></div>
        </div>
    </div>
</div>
<{include file="footer.html"}>
