DROP TABLE IF EXISTS `dcr_qy_admin`;
CREATE TABLE `dcr_qy_admin` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `username` varchar(40) NOT NULL,
  `password` varchar(40) NOT NULL,
  `logintime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `loginip` varchar(30) DEFAULT '127.0.0.1',
  `logincount` mediumint(8) unsigned DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_flink`;
CREATE TABLE `dcr_qy_flink` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `webname` varchar(50) NOT NULL,
  `weburl` varchar(100) NOT NULL,
  `logo` varchar(50) DEFAULT '',
  `addtime` int(10) unsigned DEFAULT '0',
  `updatetime` int(10) unsigned DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_hudong`;
CREATE TABLE `dcr_qy_hudong` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `type` tinyint(1) unsigned DEFAULT '1',
  `title` varchar(100) NOT NULL,
  `updatetime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `loveproduct` varchar(250) DEFAULT '',
  `realname` varchar(250) DEFAULT '',
  `tel` varchar(250) DEFAULT '',
  `address` varchar(250) DEFAULT '',
  `content` mediumtext,
  `t2` varchar(250) NOT NULL DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_hudong_field`;
CREATE TABLE `dcr_qy_hudong_field` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `orderid` int(2) unsigned DEFAULT '1',
  `itemname` varchar(100) DEFAULT '',
  `fieldname` varchar(100) DEFAULT '',
  `dtype` varchar(15) DEFAULT NULL,
  `vdefault` varchar(100) DEFAULT '',
  `maxlength` int(2) unsigned DEFAULT '250',
  `updatetime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_news`;
CREATE TABLE `dcr_qy_news` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `title` varchar(100) NOT NULL DEFAULT '',
  `classid` int(3) unsigned NOT NULL DEFAULT '0',
  `istop` tinyint(1) unsigned DEFAULT '0',
  `logo` varchar(100) DEFAULT '',
  `author` varchar(100) DEFAULT '',
  `source` varchar(100) DEFAULT '',
  `click` mediumint(8) unsigned DEFAULT '0',
  `addtime` int(10) DEFAULT '0',
  `updatetime` int(10) DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_news_addon`;
CREATE TABLE `dcr_qy_news_addon` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `aid` mediumint(8) unsigned NOT NULL,
  `content` text,
  `keywords` varchar(100) DEFAULT NULL,
  `description` varchar(200) DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_news_class`;
CREATE TABLE `dcr_qy_news_class` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `classname` varchar(100) NOT NULL,
  `classdescription` mediumtext,
  `updatetime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_product`;
CREATE TABLE `dcr_qy_product` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `title` varchar(100) NOT NULL,
  `classid` int(3) unsigned NOT NULL,
  `istop` tinyint(1) unsigned DEFAULT '0',
  `logo` varchar(100) DEFAULT '',
  `biglogo` varchar(100) DEFAULT '',
  `tags` varchar(100) DEFAULT '',
  `click` int(5) unsigned DEFAULT '0',
  `updatetime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=113 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_product_addon`;
CREATE TABLE `dcr_qy_product_addon` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `aid` mediumint(8) unsigned NOT NULL,
  `content` text,
  `keywords` varchar(100) DEFAULT NULL,
  `description` varchar(200) DEFAULT '',
  `guanlian` text COMMENT '关联产品',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=113 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_product_class`;
CREATE TABLE `dcr_qy_product_class` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `parentid` mediumint(8) unsigned NOT NULL DEFAULT '0',
  `orderid` mediumint(3) unsigned DEFAULT '1',
  `classname` varchar(100) NOT NULL,
  `classdescription` mediumtext,
  `updatetime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=10 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_single`;
CREATE TABLE `dcr_qy_single` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `title` varchar(100) NOT NULL DEFAULT '',
  `content` mediumtext,
  `updatetime` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=4 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_tagindex`;
CREATE TABLE `dcr_qy_tagindex` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `tag` varchar(20) NOT NULL,
  `click` int(10) DEFAULT '0',
  `total` int(10) DEFAULT '0',
  `addtime` int(10) unsigned DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `dcr_qy_taglist`;
CREATE TABLE `dcr_qy_taglist` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `aid` int(10) unsigned DEFAULT NULL,
  `typeid` int(10) unsigned DEFAULT NULL,
  `tag` varchar(20) DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=12 DEFAULT CHARSET=utf8;

